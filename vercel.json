{
    "version": 2,
    "framework": null,
    "functions": {
        "public/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "buildCommand": "vite build",
    "outputDirectory": "public"
}
